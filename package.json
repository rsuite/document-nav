{
  "name": "@rsuite/document-nav",
  "version": "1.0.18",
  "description": "Document navigation is automatically generated based on the HTML title (h1-h6) tag",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "CHANGELOG.md",
    "src",
    "lib"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server  --inline --progress --colors --port 3002 --host 0.0.0.0 --devtool source-map ",
    "docs": "rm -rf assets &&  NODE_ENV=production webpack ",
    "build": "rm -rf lib  && babel src --out-dir lib && cp -R src/less lib",
    "publish-docs": "node docs/gh-pages.js",
    "prepages": "npm run docs",
    "pages": "node docs/gh-pages.js",
    "predeploy": "npm run build",
    "prepublish": "npm run build",
    "release": "npm publish",
    "test": "jest && codecov",
    "coverage": "jest --colors --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsuite/document-nav"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "rsuite",
    "nav"
  ],
  "author": "undor",
  "license": "MIT",
  "homepage": "http://rsuite.github.com/document-nav",
  "dependencies": {
    "classnames": "^2.2.5",
    "dom-lib": "^1.1.1",
    "prop-types": "^15.5.10"
  },
  "peerDependencies": {
    "react": "^0.14.9 || >=15.3.0",
    "react-dom": ">=0.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-standalone": "^6.7.7",
    "codecov": "^2.2.0",
    "coveralls": "^3.0.0",
    "css-loader": "^0.23.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "es5-shim": "^4.1.14",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.62.0",
    "gh-pages": "^0.12.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.24.0",
    "includes-loader": "^0.1.0",
    "jest": "^19.0.2",
    "jsdom": "^16.0.1",
    "json-loader": "^0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "markdown-loader": "^2.0.0",
    "markdownloader": "^1.0.5",
    "react": "^16.8.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^1.3.1",
    "react-markdown": "^2.5.0",
    "react-router": "^3.0.0",
    "react-test-renderer": "^16.0.0",
    "rsuite": "^3.7.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.8",
    "uuid": "^2.0.3",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.2.0"
  }
}
